<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.somg.web.file.generator.mapper.RoleMapper">
    <select id="getRoleByRoleName" resultType="com.somg.web.file.generator.pojo.origin.Role">
        select * from role where `roleName`=#{roleName};
    </select>


</mapper>